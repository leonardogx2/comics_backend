{
  "name": "comic-books-backend",
  "version": "1.0.0",
  "description": "Projeto da etapa final rockets!!!",
  "main": "index.js",
  "scripts": {
    "seed": "tsx ./prisma/seeds/index.ts",
    "dev": "tsx watch src/shared/infra/http/server.ts",
    "start": "node dist/src/shared/infra/http/server.js",
    "build": "rimraf dist && tsc && tsc-alias && npm run coppy && npm run coppy:ssl",
    "coppy": "copyfiles -f ./src/**/resources/*.html  ./dist/shared/resources",
    "coppy:ssl": "copyfiles -f ./src/**/ssl/*  ./dist/src/shared/ssl",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "leozin",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "prisma": "^5.14.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "tsx": "^4.11.0",
    "zod": "^3.23.8"
  }
}
